{
  "name": "redux-mock-saga",
  "version": "0.1.8",
  "description": "Mock library to thoroughly test redux-sagas",
  "main": "./lib/index.js",
  "typings": "./lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/giespaepen/redux-mock-saga.git"
  },
  "scripts": {
    "build-watch": "yarn tsc --watch",
    "build": "yarn tsc && yarn generate-flowtypes",
    "generate-flowtypes": "flowgen lib/index.d.ts -o flow-typed/index.flow.js",
    "prepublishOnly": "yarn build",
    "test": "jest --config jest.config.json --no-cache && codecov -f coverage/*.json -t fbf0fd97-eaa0-4700-a62e-28ec14665604"
  },
  "keywords": [
    "redux-saga",
    "test",
    "redux"
  ],
  "author": "Gie Spaepen <giespaepen@outlook.com>",
  "license": "MIT",
  "dependencies": {
    "redux": "^3.7.2",
    "redux-saga": "^0.16.0"
  },
  "devDependencies": {
    "@types/enzyme": "^3.1.6",
    "@types/jest": "^21.1.8",
    "@types/react": "^16.0.31",
    "codecov": "^3.0.0",
    "flowgen": "^1.2.0",
    "ts-jest": "^22.0.0",
    "tslint": "^5.8.0",
    "tslint-microsoft-contrib": "^5.0.1",
    "typescript": "2.7"
  }
}