{
  "name": "redux-mock-saga",
  "version": "0.2.0",
  "description": "Mock library to thoroughly test redux-sagas",
  "main": "./lib/index.js",
  "typings": "./lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/giespaepen/redux-mock-saga.git"
  },
  "scripts": {
    "build-watch": "yarn tsc --watch",
    "build": "yarn tsc && yarn generate-flowtypes",
    "generate-flowtypes": "flowgen lib/index.d.ts -o flow-typed/index.flow.js",
    "prepublishOnly": "yarn build",
    "test": "jest --config jest.config.js --no-cache && codecov -f coverage/*.json -t fbf0fd97-eaa0-4700-a62e-28ec14665604"
  },
  "keywords": [
    "redux-saga",
    "test",
    "redux"
  ],
  "author": "Gie Spaepen <giespaepen@outlook.com>",
  "license": "MIT",
  "dependencies": {
    "redux": "4.0.4",
    "redux-saga": "^1.1.1"
  },
  "devDependencies": {
    "@types/enzyme": "^3.1.6",
    "@types/jest": "^24.0.21",
    "@types/react": "^16.0.31",
    "codecov": "^3.0.0",
    "flowgen": "^1.2.0",
    "jest": "^24.9.0",
    "ts-jest": "^24.1.0",
    "tslint": "^6.0.0-beta0",
    "tslint-microsoft-contrib": "^6.2.0",
    "typescript": "3.6.4"
  }
}