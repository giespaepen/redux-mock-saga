  },
  "devDependencies": {
    "@types/enzyme": "^3.1.6",
    "@types/jest": "^21.1.8",
    "@types/react": "^16.0.31",
    "@types/redux-saga": "^0.10.5",
    "flowgen": "^1.2.0",
    "tslint": "^5.8.0",
    "tslint-microsoft-contrib": "^5.0.1",
    "typescript": "^2.6.2"
  }
